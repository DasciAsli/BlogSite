@model List<AD_BlogProject_2021.Models.EntityFramework.Tags>

    <section class="cta-section theme-bg-light py-5">
        <div class="container text-center single-col-max-width">
            <div class="input-group">

                <input type="text" name="name" value="" id="ara" class="form-control btn-rounded"/>               
                <button type="submit" class="btn btn-outline-primary" id="araButonu">Ara</button>
              
                <div class="btn-group ">
                    <button type="button" class="btn btn-outline-primary dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
                        Etiketler
                    </button>
                    <ul class="dropdown-menu">
                        @foreach (var item in Model)
                        {
                            <li> <a class="dropdown-item" onclick="blogGetir(@(item.TagId))" href="#">@item.TagName</a></li>
                        }


                    </ul>
                </div>

            </div>
          
        </div>
        <!--//container-->
    </section>
